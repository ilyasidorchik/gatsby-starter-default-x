{
  "name": "gatsby-starter-default-x",
  "private": true,
  "description": "An extended starter to get up and developing quickly with Gatsby",
  "version": "2.1",
  "author": "Ilya Sidorchik <ilya@sidorchik.ru>",
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@bem-react/classname": "^1.5.6",
    "@bem-react/classnames": "^1.3.7",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "babel-preset-gatsby": "^0.2.26",
    "gatsby": "^4.25.7",
    "gatsby-cli": "^2.8.23",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-sharp": "^2.3.7",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "strip-ansi": "^6.0.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "husky": "^4.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-order": "^4.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilyasidorchik/gatsby-starter-default-x"
  },
  "bugs": {
    "url": "https://github.com/ilyasidorchik/gatsby-starter-default-x/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ],
    "!(_*).scss": [
      "stylelint --fix --config ./.stylelintrc",
      "git add"
    ]
  }
}
